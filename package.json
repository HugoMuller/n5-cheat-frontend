{
  "name": "n5-cheat",
  "private": false,
  "repository": "https://github.com/HugoMuller/n5-cheat-frontend.git",
  "version": "0.0.1",
  "description": "A simple cheat manager for RetroN5",
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "2.3.1",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.2.10",
    "gulp-cssnano": "^2.1.1",
    "gulp-filter": "^2.0.2",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-less": "^3.0.5",
    "gulp-load-plugins": "^0.10.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rev": "^5.0.1",
    "gulp-rev-replace": "^0.4.3",
    "gulp-uglify": "^1.2.0",
    "gulp-useref": "^3.0.0",
    "gulp-util": "^3.0.6",
    "lazypipe": "^0.2.4",
    "open": "0.0.5",
    "rimraf": "^2.4.0",
    "run-sequence": "^1.1.1",
    "wiredep": "^3.0.0"
  },
  "devDependencies": {
    "bower": "^1.7.7",
    "eslint": "^2.1.0",
    "gulp-connect": "^2.2.0",
    "gulp-watch": "^4.2.4",
    "jasmine-core": "^2.4.1",
    "jasmine-reporters": "^2.1.1",
    "jscpd": "^0.5.3",
    "karma": "^0.13.21",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.3",
    "karma-es6-shim": "^0.2.3",
    "karma-jasmine": "~0.3.0",
    "karma-junit-reporter": "^0.3.8",
    "karma-mocha": "^0.2.2",
    "karma-ng-html2js-preprocessor": "^0.2.1",
    "karma-phantomjs-launcher": "*",
    "karma-should": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-spec-reporter": "0.0.24",
    "lodash": "^4.5.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.4",
    "protractor": "^3.2.2",
    "protractor-jasmine2-screenshot-reporter": "^0.3.0",
    "should": "^8.3.0",
    "sinon": "^1.17.3"
  },
  "scripts": {
    "start": "gulp serve",
    "postinstall": "bower install",

    "test": "karma start test/unit/config/karma-dev.conf.js",
    "test-single-run": "karma start test/unit/config/karma-dev.conf.js --single-run",
    "ci-test": "karma start test/unit/config/karma-ci.conf.js --single-run",

    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update --standalone",
    "preprotractor": "npm run update-webdriver",
    "protractor": "npm run test-e2e",
    "test-e2e": "protractor test/e2e/config/protractor.conf.js",
    "ci-protractor-chrome": "protractor test/e2e/config/ci-protractor-chrome.conf.js",
    "ci-protractor-firefox": "protractor test/e2e/config/ci-protractor-firefox.conf.js",

    "preci-jshint": "mkdir -p reports",
    "ci-jshint": "eslint -f jslint-xml -o reports/jshint.xml app test *.js || true",
    "preci-cpd": "mkdir -p reports",
    "ci-cpd": "jscpd --output reports/cpd.xml",

    "clean": "rm -rf bower_components node_modules reports",
    "clean-app": "rm -rf app/bower_components",
    "clean-all": "rm -rf bower_components node_modules reports app/bower_components"
  }
}
